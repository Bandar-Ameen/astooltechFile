<!DOCTYPE html>
<html lang=%1$sen%1$s>
<head>
  <title>Bootstrap Example</title>
  <meta charset=%1$sutf-8%1$s>
  <meta name=%1$sviewport%1$s content=%1$swidth=device-width, initial-scale=1%1$s>
  <style>
@font-face{
    font-family:%1$smmy%1$s;
    src:url(themes/fonts/Cairo-Bold.ttf)format(%1$struetype%1$s);
    font-style: normal;
    }
    
    
    
    *{
     font-family: 'mmy';
     -webkit-user-select: none;
    }


</style>

<style>%3$s</style>
    <script>%4$s</script>
       <script>%5$s</script>


</head>
<body>

<div id=%1$sexample%1$s>
    <div class=%1$sdemo-section%1$s>
        <div id=%1$schat%1$s></div>
        <input name=%1$sfiles%1$s id=%1$sfiles%1$s type=%1$sfile%1$s />
        <div class=%1$sdemo-hint%1$s>You can use the Image tool in the toolbar to send an image. Or drop an image on the Chat component.</div>
    </div>

    <script>
      
function servi(gv) {
   /*var chat = $('#chat').getKendoChat();

  chat.postMessage(%1$sHello!%1$s);
  chat.postMessage(%1$sPlease, select a date from the Calendar.%1$s);
*/
var chat = $(%1$s#chat%1$s).data(%1$skendoChat%1$s);

//var chat = $(%1$s#chat%1$s).data(%1$skendoChat%1$s);

    chat.renderMessage({
        type: %1$stext%1$s,
        text: gv
    }, {
        id: kendo.guid(),
        name: %1$sSample User%1$s,
        iconUrl: %1$shttps://demos.telerik.com/kendo-ui/content/web/chat/avatar.png%1$s
    });

/*chat.renderAttachments({
        attachments: [{
            contentType: %1$sheroCard%1$s,
            content: {
                title: %1$sNinja Title%1$s,
                subtitle: %1$sThe Telerik Ninja%1$s,
                text: %1$sThis is an example.%1$s,
                images: [{
                    url: %1$shttps://docs.telerik.com/kendo-ui/images/ninja-icon.png%1$s,
