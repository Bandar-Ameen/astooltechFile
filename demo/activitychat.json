  /*chat.renderAttachments({
        attachments: [{
            contentType: %1$sheroCard%1$s,
            content: {
                title: %1$sAttachment Title%1$s,
                subtitle: %1$sAttachment Subtitle%1$s,
                text: %1$sSample text%1$s
            }
        }],
        attachmentLayout: %1$scarousel%1$s
    }, chat.getUser());
*/
    }  

  var chat;

        $(document).ready(function () {
            chat = $(%1$s#chat%1$s).kendoChat({
                toolClick: function (ev) {
                    if (ev.name === %1$ssendimage%1$s) {
                        $(%1$s#files%1$s).click();
                    }
                },
                toolbar: {
                    toggleable: true,
                    buttons: [
                        { name: %1$ssendimage%1$s, iconClass: %1$sk-icon k-i-image%1$s }
                    ]
                }
            }).data(%1$skendoChat%1$s);

            var upload = $(%1$s#files%1$s).kendoUpload({
                async: {
                    saveUrl: %1$s../upload/save%1$s,
                    removeUrl: %1$s../upload/remove%1$s,
                    autoUpload: true
                },
                validation: {
                    allowedExtensions: [%1$s.jpg%1$s, %1$s.jpeg%1$s, %1$s.png%1$s, %1$s.bmp%1$s, %1$s.gif%1$s]
                },
                success: onSuccess,
                showFileList: false,
                dropZone: %1$s#chat%1$s
            }).data(%1$skendoUpload%1$s);

            upload.wrapper.hide();
        });

        function onSuccess(e) {
            if (e.operation === %1$supload%1$s) {
                for (var i = 0; i < e.files.length; i++) {
                    var file = e.files[i].rawFile;

                    if (file) {
                        var reader = new FileReader();

                        reader.onloadend = function () {
                            chat.renderAttachments({
                                attachments: [{
                                    contentType: %1$simage_card%1$s,
                                    content: {
                                        image: this.result
                                    }
                                }],
                                attachmentLayout: %1$slist%1$s
                            }, chat.getUser());
                        };

                        reader.readAsDataURL(file);
                    }
                }
            }
        }

        var IMAGE_CARD_TEMPLATE = kendo.template(
            '<div class=%1$sk-card k-card-type-rich%1$s>' +
            '<div class=%1$sk-card-body quoteCard%1$s>' +
            '<img alt=%1$sKendo UI for jQuery Chat Toolbar Custom Tool Image%1$s class=%1$simage-attachment%1$s src=%1$s#: image #%1$s />' +
            '</div>' +
            '</div>'
        );

        kendo.chat.registerTemplate(%1$simage_card%1$s, IMAGE_CARD_TEMPLATE);
    </script>

    <style>
        .k-card .image-attachment {
            max-height: 120px;
        }
    </style>
</div>


  
