{
  "RelativeLayoutExample": {
    "type": "FrameLayout",
    "layout_width": "match_parent",
    "layout_height": "wrap_content",
    "padding": "16dp",
    "paddingBottom": "0dp",
    "children": [
      {
        "type": "RelativeLayout",
        "layout_width": "match_parent",
        "layout_height": "100dp",
        "background": "#ffffff",
        "children": [
          {
            "type": "TextView",
            "id": "center",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "text": "Center",
            "textColor": "#323232",
            "textSize": "14sp",
            "layout_centerVertical": true,
            "layout_centerHorizontal": true
          },
          {
            "type": "ImageView",
            "src": "@drawable/star_filled",
            "layout_marginBottom": "4dp",
            "layout_above": "center",
            "layout_centerHorizontal": true
          },
          {
            "type": "ImageView",
            "src": "@drawable/star_filled",
            "layout_marginTop": "4dp",
            "layout_below": "center",
            "layout_centerHorizontal": true
          },
          {
            "type": "ImageView",
            "src": "@drawable/star_filled",
            "layout_marginLeft": "4dp",
            "layout_toRightOf": "center",
            "layout_centerVertical": true
          },
          {
            "type": "ImageView",
            "src": "@drawable/star_filled",
            "layout_marginRight": "4dp",
            "layout_toLeftOf": "center",
            "layout_centerVertical": true
          },
          {
            "type": "ImageView",
            "src": "https://www.google.com/images/branding/googlelogo/2x/googlelogo_color_272x92dp.png",
            "layout_height": "70dp",
            "layout_width": "120dp",
            "padding": "4dp",
            "background": "#000000",
            "scaleType": "fitCenter",
            "layout_alignParentTop": true,
            "layout_alignParentRight": true
          }
        ]
      }
    ]
  },
  "SimpleDataBindingExample": {
    "type": "FrameLayout",
    "layout_width": "match_parent",
    "layout_height": "wrap_content",
    "padding": "16dp",
    "paddingBottom": "0dp",
    "children": [
      {
        "type": "LinearLayout",
        "layout_width": "match_parent",
        "layout_height": "wrap_content",
        "orientation": "vertical",
        "background": "#ffffff",
        "children": [
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "layout_marginBottom": "0dp",
            "text": "Profile",
            "style": "mini.blue"
          },
          {
            "type": "RelativeLayout",
            "layout_width": "match_parent",
            "layout_height": "wrap_content",
            "paddingLeft": "16dp",
            "paddingRight": "16dp",
            "paddingTop": "8dp",
            "children": [
              {
                "type": "TextView",
                "id": "name",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_marginRight": "4dp",
                "textSize": "18dp",
                "text": "@{user.name}"
              },
              {
                "type": "TextView",
                "id": "location",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_below": "name",
                "layout_marginRight": "4dp",
                "textSize": "18dp",
                "text": "@{fn:format('%s, %s', @{user.location.city}, @{user.location.country})}"
              },
              {
                "type": "TextView",
                "text": "@{fn:format('(%s)',@{user.level})}",
                "textColor": "#c48866",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_toRightOf": "name"
              }
            ]
          },
          {
            "type": "LinearLayout",
            "layout_width": "match_parent",
            "layout_height": "wrap_content",
            "orientation": "vertical",
            "paddingLeft": "16dp",
            "paddingRight": "16dp",
            "children": [
              {
                "type": "HorizontalProgressBar",
                "layout_width": "match_parent",
                "layout_height": "wrap_content",
                "layout_marginTop": "8dp",
                "max": "@{user.experience.max}",
                "progress": "@{user.experience.current}"
              },
              {
                "type": "TextView",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_marginTop": "8dp",
                "layout_marginBottom": "32dp",
                "padding": "8dp",
                "paddingTop": "16dp",
                "paddingBottom": "16dp",
                "text": "@{fn:number(@{user.credits})}",
                "style": "money.center",
                "background": {
                  "type": "shape",
                  "shape": "oval",
                  "children": [
                    {
                      "type": "gradient",
                      "startColor": "#44e73827",
                      "endColor": "@color/green",
                      "angle": 270
                    }
                  ]
                },
                "animation": {
                  "interpolator": {
                    "type": "anticipateOvershootInterpolator"
                  },
                  "type": "rotate",
                  "fromDegrees": -720,
                  "toDegrees": 0,
                  "pivotX": "50%",
                  "pivotY": "50%",
                  "duration": 1500
                }
              }
            ]
          },
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "textSize": "14sp",
            "data": {
              "date": "@{fn:date(@{sec.ll},'E, d MMM')}",
              "loc": "@{sec.loc}"
            },
            "text": "@{fn:format('last login: %s from %s',@{date},@{loc})}",
            "style": "blue"
          },
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "textSize": "14sp",
            "text": "@{user.type}",
            "visibility": "@{fn:ternary(@{user.type}, 'visible', 'gone')}"
          }
        ]
      }
    ]
  },
  "DataDrivenExample": {
    "type": "FrameLayout",
    "layout_width": "match_parent",
    "layout_height": "wrap_content",
    "padding": "16dp",
    "paddingBottom": "0dp",
    "children": [
      {
        "type": "LinearLayout",
        "orientation": "horizontal",
        "layout_width": "match_parent",
        "background": "#ffffff",
        "padding": "16dp",
        "children": {
          "@": {
            "collection": "@{fn:slice(@{user.tags},0,4)}",
            "layout": {
              "type": "LinearLayout",
              "data": {
                "tag": "@{user.tags[$index]}"
              },
              "layout_marginRight": "8dp",
              "layout_height": "32dp",
              "textSize": "12sp",
              "textColor": "@color/colorAccent",
             "children":[{
                       "type": "TextView",
              "text": "@{tag}",
              "padding": "8dp",
              "gravity": "center",
              "onClick": "@{tag}"
}]


            }
          }
        }
      }
    ]
  },
  
