  "RecyclerViewExample": {
    "type": "FrameLayout",
    "layout_width": "match_parent",
    "layout_height": "wrap_content",
    "padding": "16dp",
    "paddingBottom": "0dp",
    "children": [
      {
        "type": "RecyclerView",
        "layout_width": "match_parent",
        "layout_height": "148dp",
        "background": "#ffffff",
        "data": {
          "items": "@{data.achievements}"
        },
        "layout_manager": {
   "type": "LinearLayoutManager",
            "orientation":"vertical",
            "reverse":false,
            "space":"10"
        },
        "adapter": {
          "@": {
            "type": "SimpleListAdapter",
            "item-count": "@{items.$length}",
            
            "item-layout": {
            
            
          "type": "include",
          "layout": "DataDrivenExamplev",
    
          "paddingBottom": "0dp",
          "data": {
            "user": "@{items}",
                  "my": "@{items[$index]}"
          }
            
              



                      
              
              
              
      
            }
          }
        }
      }
    ]
  },
  "AlertDialogLayout": {
    "type": "LinearLayout",
    "orientation": "vertical",
    "layout_width": "80dp",
    "layout_height": "wrap_content",
    "padding": "20dp",
    "children": [
      {
        "type": "TextView",
        "layout_width": "match_parent",
        "layout_height": "wrap_content",
        "layout_marginTop": "16dp",
        "gravity": "center",
        "textColor": "#ffaa77",
        "textSize": "32sp",
        "text": "@{s0.my[0].data}"
      },
      {
        "type": "TextView",
        "layout_width": "match_parent",
        "layout_height": "wrap_content",
        "gravity": "center",
        "textSize": "22sp",
        "text": "press ok to continue."
      }
    ]
  }
}