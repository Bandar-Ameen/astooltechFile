  "SimpleDataBindingExample": {
    "type": "FrameLayout",
    "layout_width": "match_parent",
    "layout_height": "wrap_content",
    "padding": "16dp",
    "paddingBottom": "0dp",
    "children": [
      {
        "type": "LinearLayout",
        "layout_width": "match_parent",
        "layout_height": "wrap_content",
        "orientation": "vertical",
        "background": "#ffffff",
        "children": [
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "layout_marginBottom": "0dp",
            "text": "Profile",
            "style": "mini.blue"
          },
          {
            "type": "RelativeLayout",
            "layout_width": "match_parent",
            "layout_height": "wrap_content",
            "paddingLeft": "16dp",
            "paddingRight": "16dp",
            "paddingTop": "8dp",
            "children": [
              {
                "type": "TextView",
                "id": "k",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_marginRight": "4dp",
                "textSize": "18dp",
                "text": "@{name}"
              },
              {
                "type": "TextView",
                "id": "location",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_below": "name",
                "layout_marginRight": "4dp",
                "textSize": "18dp",
                "text": "@{fn:format('%s, %s', @{user.location.city}, @{user.location.country})}"
              },
              {
                "type": "TextView",
                "text": "@{fn:format('(%s)',@{user.level})}",
                "textColor": "#c48866",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_toRightOf": "name"
              }
            ]
          },
          {
            "type": "LinearLayout",
            "layout_width": "match_parent",
            "layout_height": "wrap_content",
            "orientation": "vertical",
            "paddingLeft": "16dp",
            "paddingRight": "16dp",
            "children": [
              {
                "type": "HorizontalProgressBar",
                "layout_width": "match_parent",
                "layout_height": "wrap_content",
                "layout_marginTop": "8dp",
                "max": "@{user.experience.max}",
                "progress": "@{user.experience.current}"
              },
              {
                "type": "TextView",
                "layout_width": "wrap_content",
                "layout_height": "wrap_content",
                "layout_marginTop": "8dp",
                "layout_marginBottom": "32dp",
                "padding": "8dp",
                "paddingTop": "16dp",
                "paddingBottom": "16dp",
                "text": "@{fn:number(@{user.credits})}",
                "style": "money.center",
                "background": {
                  "type": "shape",
                  "shape": "oval",
                  "children": [
                    {
                      "type": "gradient",
                      "startColor": "#44e73827",
                      "endColor": "@color/green",
                      "angle": 270
                    }
                  ]
                },
                "animation": {
                  "interpolator": {
                    "type": "anticipateOvershootInterpolator"
                  },
                  "type": "rotate",
                  "fromDegrees": -720,
                  "toDegrees": 0,
                  "pivotX": "50%",
                  "pivotY": "50%",
                  "duration": 1500
                }
              }
            ]
          },
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "textSize": "14sp",
            "data": {
              "date": "@{fn:date(@{sec.ll},'E, d MMM')}",
              "loc": "@{sec.loc}"
            },
            "text": "@{fn:format('last login: %s from %s',@{date},@{loc})}",
            "style": "blue"
          },
          {
            "type": "TextView",
            "layout_width": "wrap_content",
            "layout_height": "wrap_content",
            "textSize": "14sp",
            "text": "@{user.type}",
            "visibility": "@{fn:ternary(@{user.type}, 'visible', 'gone')}"
          }
        ]
      }
    ]
  },
  
  
